{
  "expo": {
    "name": "ColorLife Studio",
    "slug": "colorlife-studio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.lucasfmp.colorlifestudio"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O app precisa acessar suas fotos para importar a foto para a an√°lise."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7a74fc1d-da52-4bc4-a5d1-a50355b49c2b"
      }
    }
  }
}
